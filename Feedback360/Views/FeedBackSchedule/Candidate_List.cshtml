@model IEnumerable<RepoFeedback360.Model.ScheduledCandidatesDL>

@{
    ViewData["Title"] = "Candidate_List";
}

<h1>Candidate List</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Candidate_UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Employee_Name)
            </th>
            @* <th>
                @Html.DisplayNameFor(model => model.EmployeeId_To)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmployeeId_By)
            </th> *@
            <th>
                @Html.DisplayNameFor(model => model.Designation_Id)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Candidate_UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Employee_Name)
            </td>
           @*  <td>
                @Html.DisplayFor(modelItem => item.EmployeeId_To)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeId_By)
            </td> *@
            <td>
                @Html.DisplayFor(modelItem => item.Designation_Id)
            </td>
            <td>
                @Html.ActionLink("Provide FeedBack","ProvideFeedBack","FeedBackSchedule",new {
                  
                    EmployeeId_To= item.EmployeeId_To,
                    EmployeeId_By= item.EmployeeId_By,
                    DesignationId = item.Designation_Id
                  
                }) 
                
            </td>
        </tr>
}
    </tbody>
</table>
